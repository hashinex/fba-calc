{
  "manifest_version": 3,
  "name": "AMZCALC - FBA Profit Calculator",
  "version": "1.0",
  "description": "Clone of SellerAMP extension - FBA fee and profit calculator",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
